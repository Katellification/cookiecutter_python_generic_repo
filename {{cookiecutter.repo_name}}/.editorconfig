# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Default to all files
[*]
end_of_line = crlf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{json,yml,yaml,template,md}]
indent_style = space
indent_size = 2

[*.{ps1,psm1}]
indent_style = space
indent_size = 4
